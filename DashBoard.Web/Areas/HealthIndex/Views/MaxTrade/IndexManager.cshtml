@model DashBoard.Web.Areas.HealthIndex.Models.IndexManagers
@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <title></title>
    <script type="text/javascript" src="~/Scripts/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/healthindexconfig.js"></script>
</head>
<body>
    <form id="HealthIndexConfig" method="post" action="/DashBoard/MaxTrade/ChangeConfig">

        <table align="left" style="margin-top:50px;" cellpadding="10" cellspacing="10">
            <tbody>
                <tr>
                    <td align="left">
                        日交易量最大规模:
                    </td>
                    <td>
                        <input id="MaxDayTrade" type="number" size="5" value="@ViewBag.MaxDay" name="MaxDay" />
                    </td>
                </tr>
                <tr>
                    <td>
                        每分钟交易量最大规模:
                    </td>
                    <td>
                        <input id="MaxMinuteTrade" type="number" size="5" value="@ViewBag.MaxMinute" name="MaxMinute" />
                    </td>
                </tr>
                <tr>
                    <td>
                        每秒交易量最大规模:
                    </td>
                    <td>
                        <input id="MaxSecondTrade" type="number" size="5" value="@ViewBag.MaxSecond" name="MaxSecond" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <input id="saveconfig" type="button" value="保存">
                    </td>
                </tr>
            </tbody>
        </table>
    </form>
</body>
</html>