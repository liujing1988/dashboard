@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="~/Content/jq.css" type="text/css" media="print, projection, screen" />
    <link rel="stylesheet" href="~/Content/style.css" type="text/css" media="print, projection, screen" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/daterangepicker-1.3.7.css">
    <link rel="stylesheet" href="~/Content/daterangepicker-bs3.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <script type="text/javascript" src="~/Scripts/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/highcharts.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/exporting.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/Export-csv.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/RealTimeTradeAmount.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/grid.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/MonthTradeAmount.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/TradeDetails.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/moment.js"></script>
    <script type="text/javascript" src="~/Scripts/daterangepicker-1.3.7.js"></script>
    <title>国泰君安量化交易监控系统</title>
</head>
<body>
    <table width="98%" align="center" cellpadding="1.5%">
        <tbody>
            <tr>
                <td colspan="2">
                    <a name="trade" />
                    <label><font size="4" color="black">月交易情况:</font></label>
                </td>
            </tr>
            <tr>
                <td valign="middle">
                    <div id="getMonth_Trade" class="pull-left dateRange" style="margin-left: 0px">
                        日期：<i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
                        <span id="searchDateRange"></span>
                        <b class="caret"></b>
                    </div>
                    <br>
                </td>
                <td>
                    <div>
                        策略名称:<input id="strategyname" type="text" />
                        策略描述:<input id="stratinfo" type="text" />
                        序列号:<input id="seriesno" type="text" />
                    </div>
                </td>
                <td align="right">
                    <font size="2">单位：</font>
                    <select id="unit">
                        <option value="001">元</option>
                        <option value="002">万元</option>
                        <option value="003">亿元</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td colspan="3">
                    <div id="MonthTrade" style="width:100%;height:40%"></div>
                    <div id="divChart" style="position: absolute; display: -none; border: 3px solid green; z-index: 999;"></div>
                </td>
            </tr>
            <tr>
                <td colspan="2" height="10px"></td>
            </tr>
            <tr>
                <td colspan="3">
                    <a name="realtime" />
                    <label><font size="4" color="black">实时交易情况:</font></label>
                </td>
            </tr>
            <tr>
                <td height="10px"></td>
            </tr>
            <tr>
                <td colspan="3">
                    <div id="realtimeTrade" style="width:100%;height:40%"></div>
                </td>
            </tr>
        </tbody>
    </table>
    </body>
</html>

