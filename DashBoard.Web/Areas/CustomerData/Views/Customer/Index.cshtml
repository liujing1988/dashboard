<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="~/Content/jq.css" type="text/css" media="print, projection, screen" />
    <link rel="stylesheet" href="~/Content/style.css" type="text/css" media="print, projection, screen" />
    <link rel="stylesheet" href="~/Content/jquery.dataTables.min.css">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    @*<link rel="stylesheet" href="~/Content/daterangepicker-1.3.7.css">*@
    <link rel="stylesheet" href="~/Content/daterangepicker-bs3.css">
    <link rel="stylesheet" href="~/Content/font-awesome.min.css">
    <script type="text/javascript" src="~/Scripts/highcharts/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/highcharts.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/exporting.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/Export-csv.js"></script>
    <script type="text/javascript" src="~/Scripts/highcharts/grid.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/moment.js"></script>
    <script type="text/javascript" src="~/Scripts/daterangepicker-1.3.7.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/StrategyType_use.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/Customer.js"></script>
    <script type="text/javascript" src="~/Scripts/DashBoard/TradeDetails.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <label><font color="#FFFFFF" face="黑体" size="6">国泰君安量化交易情况统计分析系统</font></label>
            </div>
            <br>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("首页", "Index", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("策略明细", "Index", "TradeData", new { area = "TradeData" }, null)</li>
                    <li>@Html.ActionLink("客户明细", "Index", "Customer", new { area = "CustomerData" }, null)</li>
                    <li>@Html.ActionLink("综合查询", "Index", "Customer", new { area = "CustomerData" }, null)</li>
                    <li>@Html.ActionLink("平台健康指数", "Index", "Customer", new { area = "CustomerData" }, null)</li>
                </ul>
            </div>
        </div>
    </div>
    <table width="80%" align="center" border="1" cellspacing="10" cellpadding="10">
            <tbody>
                <tr>
                    <td>
                        <a name="customer" />
                        <label><font size="4" color="black">客户增长情况:</font></label>
                    </td>
                    <td>
                        <label><font size="4" color="black">模块使用情况:</font></label>
                    </td>
                </tr>
                <tr>
                    <td width="43%">
                        <div id="getMonth_Customer" class="pull-left dateRange" style="margin-left: 0px">
                            日期：<i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
                            <span id="searchDateRange"></span>
                            <b class="caret"></b>
                        </div>
                        <br>
                    </td>
                    <td valign="middle" width="43%">
                        <div id="getDate_Type" class="pull-left dateRange" style="margin-left: 0px">
                            日期：<i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
                            <span id="searchDateRange"></span>
                            <b class="caret"></b>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div id="column" style="height:40%;"></div>
                    </td>
                    <td>
                        <div id="pie" style="height:40%"></div>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <a name="detail" />
                        <label><font size="4" color="black">客户交易流水:</font></label>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <table id="myDataTable" class="table table-striped table-bordered" width="100%">
                            <thead>
                                <tr>
                                    <th>客户ID</th>
                                    <th>交易日期</th>
                                    <th>股票代码</th>
                                    <th>交易量</th>
                                    <th>交易价格</th>
                                    <th>交易方向</th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>客户ID</th>
                                    <th>交易日期</th>
                                    <th>股票代码</th>
                                    <th>交易量</th>
                                    <th>交易价格</th>
                                    <th>交易方向</th>
                                </tr>
                            </tfoot>
                        </table>
                    </td>
                </tr>
            </tbody>
    </table>
</body>
</html>

